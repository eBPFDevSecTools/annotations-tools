{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Human annotations schema",
    "description": "Schema for Human Annotations of Functions"
    "type": "object",
    "properties": {
      "gitRepoUrl": {
        "description": "Url of Git repo of code"
        "type": "string"
      },
      "commitId": {
        "description": "Commit id of code pulled from git repo"
        "type": "string"
      },
      "type":"array",
      "minItems": 1
	    "items":{
	    "type":"object",
	    "properties":{
		"filePath": {
         "description": "File path relative to root of git_repo",
        "type": "string"
      },
      "description": {
        "description": "Human description of function",
        "type": "string"
      },
      "author": {
        "description": "Name of author",
        "type": "string"
      },
      "authorEmail": {
        "description": "Email of author",
        "type": "string"
      },
      "date": {
        "description": "Date of creation of human annotation",
        "type": "string"
      },
      "funcName": {
        "description": "Name of function",
        "type": "string"
      },
      "startLine": {
          "description": "start	line of function",
        "type": "integer"
      },
      "endLine": {
        "description": "end line of function",
        "type": "integer"
      },

      "required": ["filePath","description","author","authorEmail","date","funcName"]
    },
    
	    }
      "required": ["gitRepoUrl", "commitId"]
	},
      
}
